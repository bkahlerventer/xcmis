               <include>**/*.conf</include>
               <include>**/*.config</include>
               <include>**/*.groovy</include>
               <include>**/*.wsdl</include>
            </includes>
         </testResource>
         <testResource>
            <directory>src/test/resources</directory>
            <includes>
               <include>**/*.properties</include>
               <include>**/*.xml</include>
               <include>**/*.xsl</include>
               <include>**/*.xsd</include>
